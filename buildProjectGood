#komentarzaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa

var temp1 = 'hello';
var temp2 = 'false';
var moduleFlags = '-Wall';

var headerLibFiles = ['header.o', 'aaaaaa.txt', 'factoraal.o']; #komentarz2
build('aaa', ['bbb', 'ccc']);
build('bbb', ['ddd', 'eee']);
build('ddd', ['fff', 'uuu']);

staticlib('header.a', headerLibFiles, moduleFlags);

def buildWithFlags(list1, list2) {
    var jou = 'jou';
	if(jou == 'aaa' || temp2 == 'false') {
		for(o in list1, jou in list2) {
			build(o,jou,moduleFlags);
		}
	} else {
		var jou = '0';
		build(jou, jou);
	}
}

call buildWithFlags(['h.o', 'f.o'], ['bbb', ['aaa.tx', 'eee.txt']]);